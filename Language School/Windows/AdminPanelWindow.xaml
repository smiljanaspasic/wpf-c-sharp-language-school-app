        <Button Name="btnProfil"
                Width="100"
                Height="45"
                 Margin="0,22,182,351" HorizontalAlignment="Right" RenderTransformOrigin="1.127,0.502" Click="btnProfil_Click">
            Pregled Profila
        </Button>
        <Button Name="btnLogout"
